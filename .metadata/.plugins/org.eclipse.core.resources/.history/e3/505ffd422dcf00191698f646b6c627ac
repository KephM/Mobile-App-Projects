package identify_yourself_kephryn;

/**
 * @author kephryn.merkley
 * Identify yourself
 * 9-4-19
 */
public class Schedule {
	
	public Schedule() {
		// this is my blue day schedule from first to fifth period
		System.out.println("Blue day schedule");
		System.out.println("Blue day | no first period");
		System.out.println("Blue day | no second period");
		System.out.println("Blue day | no third period");
		System.out.println("Blue day | Programming Python");
		System.out.println("Blue day | Programming Python");
		
		//this is my silver day schedule from first to fifth period
		System.out.println("Silver day schedule");
		System.out.println("Silver day | Mobile App Development");
		System.out.println("Silver day | Mobile App Development");
		System.out.println("Silver day | Digital Arts Intro");
		System.out.println("Silver day | Credit Recovery");
		System.out.println("Silver day | no fifth period");
	}

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		
		new Schedule();
	}

}
